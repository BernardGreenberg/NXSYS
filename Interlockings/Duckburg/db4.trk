(relay 1746D 46HV)
(relay 2D (OR (AND 3RWP 24HV)(AND 3NWP 2741HV)))
(relay 4D (OR (AND 5NWP 24HV)(AND 5RWP 2741HV)))
(relay 6D 2HV)
(relay 8D 1738HV)
(relay 22D 12HV)
(relay 28D (SWIF 27 3739HV 8HV))
(relay 30D 1746HV)
(relay 32D 30HV)
(relay 50D 40HV)
(relay 52D 50HV)
(relay 54D 22HV)
(relay 56D 48D)
